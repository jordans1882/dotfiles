{
   "applications": {
      "gecko": {
         "id": "languagetool-webextension@languagetool.org"
      }
   },
   "author": "Daniel Naber",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "browser_style": false,
      "default_icon": "images/icon48.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_tooltip__"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Space"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "contentscript.js", "markup.js", "tools.js", "purify.min.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ]
   }, {
      "js": [ "contentscript-lt.js" ],
      "matches": [ "https://languagetool.org/*", "https://www.languagetool.org/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "homepage_url": "https://languagetool.org",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "LanguageTool - Grammar and Style Checker",
   "offline_enabled": false,
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "storage", "contextMenus" ],
   "short_name": "LanguageTool",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.13"
}
